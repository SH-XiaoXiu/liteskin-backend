<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xiuxius.mc.liteskin.mapper.LiteskinProfileMapper">

    <select id="selectListByAccountId" resultType="cn.xiuxius.mc.liteskin.entity.LiteskinProfile"
            parameterType="long">
        select id,
               uuid,
               name,
               account_id,
               create_time,
               update_time
        from liteskin_profile
        where account_id = #{accountId}
    </select>
</mapper>
