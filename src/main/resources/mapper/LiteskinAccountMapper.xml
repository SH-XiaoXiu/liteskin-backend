<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xiuxius.mc.liteskin.mapper.LiteskinAccountMapper">

    <select id="selectByEmail" resultType="cn.xiuxius.mc.liteskin.entity.LiteskinAccount"
            parameterType="java.lang.String">
        select id,
               email,
               display_name,
               create_time,
               update_time
        from liteskin_account
        where email = #{email}
    </select>
</mapper>
